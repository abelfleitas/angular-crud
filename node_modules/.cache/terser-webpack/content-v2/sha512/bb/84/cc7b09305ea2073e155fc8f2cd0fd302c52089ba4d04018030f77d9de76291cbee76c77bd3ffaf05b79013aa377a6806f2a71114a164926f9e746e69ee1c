tes(svg, config.options);\n                config.svgElement = svg;\n            }\n            return config.svgElement;\n        }\n        /** Tries to create an icon config through the registered resolver functions. */\n        _getIconConfigFromResolvers(namespace, name) {\n            for (let i = 0; i < this._resolvers.length; i++) {\n                const result = this._resolvers[i](name, namespace);\n                if (result) {\n                    return isSafeUrlWithOptions(result) ?\n                        new SvgIconConfig(result.url, null, result.options) :\n                        new SvgIconConfig(result, null);\n                }\n            }\n            return undefined;\n        }\n    }\n    MatIconRegistry.ɵfac = function MatIconRegistry_Factory(t) { return new (t || MatIconRegistry)(ɵɵinject(http_HttpClient, 8), ɵɵinject(platform_browser_DomSanitizer), ɵɵinject(common_DOCUMENT, 8), ɵɵinject(ErrorHandler)); };\n    MatIconRegistry.ɵprov = /*@__PURE__*/ ɵɵdefineInjectable({ factory: function MatIconRegistry_Factory() { return new MatIconRegistry(ɵɵinject(http_HttpClient, 8), ɵɵinject(platform_browser_DomSanitizer), ɵɵinject(common_DOCUMENT, 8), ɵɵinject(ErrorHandler)); }, token: MatIconRegistry, providedIn: \"root\" });\n    return MatIconRegistry;\n})();\n/** @docs-private */\nfunction ICON_REGISTRY_PROVIDER_FACTORY(parentRegistry, httpClient, sanitizer, errorHandler, document) {\n    return parentRegistry || new icon_MatIconRegistry(httpClient, sanitizer, document, errorHandler);\n}\n/** @docs-private */\nconst ICON_REGISTRY_PROVIDER = {\n    // If there is already an MatIconRegistry available, use that. Otherwise, provide a new one.\n    provide: icon_MatIconRegistry,\n    deps: [\n        [/*@__PURE__*/ new Optional(), /*@__PURE__*/ new SkipSelf(), icon_MatIconRegistry],\n        [/*@__PURE__*/ new Optional(), http_HttpClient],\n        platform_browser_DomSanitizer,\n        ErrorHandler,\n        [/*@__PURE__*/ new Optional(), common_